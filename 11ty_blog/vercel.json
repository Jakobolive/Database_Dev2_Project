{
    "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "zeroConfig": true }
    }
  ],
    "cleanUrls": true,

    "routes": [
      { "src": "/.*", "dest": "/index.html" }
    ],
    "functions": {
      "api/server.js": { "runtime": "nodejs" }
    }
  }