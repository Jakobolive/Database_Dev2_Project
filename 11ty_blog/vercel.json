{
  "version": 2,
  "builds": [
    {
      "src": "11ty_blog/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/11ty_blog/_site/$1"
    }
  ]
}

